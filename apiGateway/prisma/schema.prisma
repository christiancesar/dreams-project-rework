generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {

  id String @id @default(auto()) @map("_id") @db.ObjectId

  email String @unique

  first_name String

  last_name String

  age Int

  birthday String

}

model Flight {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  itineraries Json
  price Json
}